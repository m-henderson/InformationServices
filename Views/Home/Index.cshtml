@model IEnumerable<InformationServices.Models.Census>
<div class="alert alert-warning">
    <p>ticket counts will reset every year</p>
</div>
<div class="container">
    <h2>Ticket Totals</h2>
    <p>This table represents tickets created in the current year</p>
   
    <table class="table">
        <thead>
        <tr>
            <th>Month</th>
            <th>Ticket Count</th>
            <th>Call Backs</th>
            <th>Year</th>
        </tr>
        </thead>
        <tbody>
        <tr>
            <td>January</td>
            <td>@ViewData["JanuaryTicketCount"]</td>
            <td>@ViewData["JanuaryCallBacks"]</td>
            <td>@ViewData["CurrentYear"]</td>
        </tr>
        <tr>
            <td>February</td>
            <td>@ViewData["FebruaryTicketCount"]</td>
            <td>@ViewData["FebruaryCallBacks"]</td>
            <td>@ViewData["CurrentYear"]</td>
        </tr>
        <tr>
            <td>March</td>
            <td>@ViewData["MarchTicketCount"]</td>
            <td>@ViewData["MarchCallBacks"]</td>
            <td>@ViewData["CurrentYear"]</td>
        </tr>
        <tr>
            <td>April</td>
            <td>@ViewData["AprilTicketCount"]</td>
            <td>@ViewData["AprilCallBacks"]</td>
            <td>@ViewData["CurrentYear"]</td>
        </tr>
        <tr>
            <td>May</td>
            <td>@ViewData["MayTicketCount"]</td>
            <td>@ViewData["MayCallBacks"]</td>
            <td>@ViewData["CurrentYear"]</td>
        </tr>
        <tr>
            <td>June</td>
            <td>@ViewData["JuneTicketCount"]</td>
            <td>@ViewData["JuneCallBacks"]</td>
            <td>@ViewData["CurrentYear"]</td>
        </tr>
        <tr>
            <td>July</td>
            <td>@ViewData["JulyTicketCount"]</td>
            <td>@ViewData["JulyCallBacks"]</td>
            <td>@ViewData["CurrentYear"]</td>
        </tr>
        <tr>
            <td>August</td>
            <td>@ViewData["AugustTicketCount"]</td>
            <td>@ViewData["AugustCallBacks"]</td>
            <td>@ViewData["CurrentYear"]</td>
        </tr>
        <tr>
            <td>September</td>
            <td>@ViewData["SeptemberTicketCount"]</td>
            <td>@ViewData["SeptemberCallBacks"]</td>
            <td>@ViewData["CurrentYear"]</td>
        </tr>
        <tr>
            <td>October</td>
            <td>@ViewData["OctoberTicketCount"]</td>
            <td>@ViewData["OctoberCallBacks"]</td>
            <td>@ViewData["CurrentYear"]</td>
        </tr>
        <tr>
            <td>November</td>
            <td>@ViewData["NovemberTicketCount"]</td>
            <td>@ViewData["NovemberCallBacks"]</td>
            <td>@ViewData["CurrentYear"]</td>
        </tr>
        <tr>
            <td>December</td>
            <td>@ViewData["DecemberTicketCount"]</td>
            <td>@ViewData["DecemberCallBacks"]</td>
            <td>@ViewData["CurrentYear"]</td>
        </tr>
        </tbody>
    </table>
</div>
<div class="container">
    <h2>Census Data</h2>
       <table class="table">
           <thead>
           <tr>
               <th>Department</th>
               <th>Patient Count</th>
               <th>Date</th>
           </tr>
           </thead>
           <tbody>
           
           @foreach (var data in Model)
           {
               <tr>
                   <td>@data.Department</td>
                   <td>@data.PatientCount</td>
                   <td>@data.DateTime.ToString("d")</td>
               </tr>
           }
           </tbody>
       </table>
   
</div>